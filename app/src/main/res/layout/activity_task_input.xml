<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:background="@drawable/gradient_with_blur">


	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/constraintLayout2"
		android:layout_width="413dp"
		android:layout_height="58dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent">


		<Toolbar
			android:id="@+id/toolbar"
			android:layout_width="match_parent"
			android:layout_height="?attr/actionBarSize"
			android:background="#F5BB66"
			app:elevation="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:title="My Toolbar"
			app:titleTextColor="@android:color/white"
			tools:ignore="MissingConstraints" />

		<ImageView
			android:id="@+id/imageView16"
			android:layout_width="51dp"
			android:layout_height="44dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/toolbar"
			app:srcCompat="@drawable/duckblack" />


		<ImageView
			android:id="@+id/imageView12"
			android:layout_width="18dp"
			android:layout_height="17dp"
			android:layout_marginStart="37dp"
			android:layout_marginBottom="29dp"
			app:layout_constraintBottom_toBottomOf="@+id/toolbar"
			app:layout_constraintStart_toStartOf="@+id/toolbar"
			app:srcCompat="@drawable/home"
			tools:ignore="MissingConstraints" />

		<TextView
			android:id="@+id/toolbarText"
			android:layout_width="60dp"
			android:layout_height="8dp"
			android:layout_gravity="center"
			android:layout_marginStart="16dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Dashboard"
			android:textColor="@android:color/black"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent" />

		<ImageView
			android:id="@+id/imageView15"
			android:layout_width="16dp"
			android:layout_height="16dp"
			android:layout_marginBottom="4dp"
			app:layout_constraintBottom_toTopOf="@+id/textView13"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.275"
			app:layout_constraintStart_toStartOf="parent"
			app:srcCompat="@drawable/moon" />

		<TextView
			android:id="@+id/textView13"
			android:layout_width="38dp"
			android:layout_height="9dp"
			android:layout_marginStart="22dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Tasks"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toEndOf="@+id/toolbarText" />


		<TextView
			android:id="@+id/textView14"
			android:layout_width="53dp"
			android:layout_height="13dp"
			android:layout_marginEnd="13dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Habits"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/textView15" />

		<ImageView
			android:id="@+id/imageView17"
			android:layout_width="15dp"
			android:layout_height="16dp"
			android:layout_marginStart="114dp"
			android:layout_marginBottom="3dp"
			app:layout_constraintBottom_toTopOf="@+id/textView14"
			app:layout_constraintStart_toStartOf="@+id/imageView16"
			app:srcCompat="@drawable/habituals" />

		<TextView
			android:id="@+id/textView15"
			android:layout_width="44dp"
			android:layout_height="12dp"
			android:layout_marginEnd="28dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Expense"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent" />

		<ImageView
			android:id="@+id/imageView18"
			android:layout_width="15dp"
			android:layout_height="16dp"
			android:layout_marginEnd="44dp"
			android:layout_marginBottom="32dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:srcCompat="@drawable/expense" />


	</androidx.constraintlayout.widget.ConstraintLayout>

	<ImageView
		android:id="@+id/imageView26"
		android:layout_width="63dp"
		android:layout_height="54dp"
		android:layout_marginStart="16dp"
		android:layout_marginTop="16dp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/burgerreal" />

	<ImageView
		android:id="@+id/imageView27"
		android:layout_width="37dp"
		android:layout_height="18dp"
		android:layout_marginTop="16dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.922"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/bell" />

	<Button
		android:id="@+id/button6"
		android:layout_width="wrap_content"
		android:layout_height="33dp"
		android:background="@drawable/mybutton"
		android:text="Save"
		android:textSize="30px"
		app:layout_constraintBottom_toTopOf="@+id/imageView30"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toEndOf="@+id/button4"
		app:layout_constraintTop_toBottomOf="@+id/groupedContainer2" />

	<Button
		android:id="@+id/button4"
		android:layout_width="wrap_content"
		android:layout_height="33dp"
		android:layout_marginStart="92dp"
		android:background="@drawable/mybutton"
		android:text="Cancel"
		android:textSize="30px"
		app:layout_constraintBottom_toTopOf="@+id/imageView30"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/groupedContainer2" />

	<TextView
		android:id="@+id/textView38"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="40dp"
		android:text="Tasks"
		android:textColor="@color/white"
		android:textSize="60px"
		android:textStyle="bold|italic"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView30" />

	<RelativeLayout
		android:id="@+id/groupedContainer2"
		android:layout_width="320dp"
		android:layout_height="201dp"
		android:layout_margin="16dp"
		android:layout_marginStart="75dp"
		android:layout_marginEnd="42dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView38"
		tools:ignore="MissingConstraints">


		<ImageView
			android:id="@+id/rectangleBackground2"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#F5BB66"
			android:scaleType="fitXY" />

		<TextView
			android:id="@+id/textView36"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="26dp"
			android:layout_marginTop="143dp"
			android:text="Description:" />

		<TextView
			android:id="@+id/textView39"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="24dp"
			android:layout_marginTop="85dp"
			android:text="Category:" />

		<EditText
			android:id="@+id/editTextText3"
			android:layout_width="123dp"
			android:layout_height="17dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_marginTop="144dp"
			android:layout_marginEnd="50dp"
			android:ems="10"
			android:inputType="text"
			android:text=""
			app:layout_constraintEnd_toEndOf="parent" />

		<EditText
			android:id="@+id/editTextText"
			android:layout_width="123dp"
			android:layout_height="17dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_marginTop="24dp"
			android:layout_marginEnd="54dp"
			android:ems="10"
			android:inputType="text"
			android:text=""
			app:layout_constraintEnd_toEndOf="parent" />

		<TextView
			android:id="@+id/textView34"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="22dp"
			android:layout_marginTop="24dp"
			android:text="Tasks Name:" />

		<Spinner
			android:id="@+id/spinner5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_marginTop="79dp"
			android:layout_marginEnd="58dp" />

	</RelativeLayout>

	<pl.droidsonroids.gif.GifImageView
		android:id="@+id/imageView30"
		android:layout_width="253dp"
		android:layout_height="205dp"
		android:layout_marginTop="80dp"
		android:src="@drawable/duckduck"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.506"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/groupedContainer2" />

	<TextView
		android:id="@+id/textView30"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="28dp"
		android:layout_marginTop="32dp"
		android:text="DuckTiveTwo"
		android:textColor="@color/white"
		android:textSize="60px"
		android:textStyle="bold|italic"
		app:layout_constraintStart_toEndOf="@+id/imageView26"
		app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>