<?xml version="1.0" encoding="utf-8"?>



<ScrollView
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:fillViewport="true">




<androidx.constraintlayout.widget.ConstraintLayout
	android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:background="@drawable/gradient_with_blur">


	<TextView
		android:id="@+id/textView42"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="136dp"
		android:layout_marginTop="44dp"
		android:text="DuckTiveTwo"
		android:textColor="@color/white"
		android:textSize="60px"
		android:textStyle="bold|italic"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/constraintLayout2"
		android:layout_width="413dp"
		android:layout_height="58dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent">


		<Toolbar
			android:id="@+id/toolbar"
			android:layout_width="match_parent"
			android:layout_height="?attr/actionBarSize"
			android:background="#F5BB66"
			app:elevation="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:title="My Toolbar"
			app:titleTextColor="@android:color/white"
			tools:ignore="MissingConstraints" />

		<ImageView
			android:id="@+id/imageView16"
			android:layout_width="51dp"
			android:layout_height="44dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="@+id/toolbar"
			app:srcCompat="@drawable/duckblack" />


		<ImageView
			android:id="@+id/imageView12"
			android:layout_width="18dp"
			android:layout_height="17dp"
			android:layout_marginStart="37dp"
			android:layout_marginBottom="29dp"
			app:layout_constraintBottom_toBottomOf="@+id/toolbar"
			app:layout_constraintStart_toStartOf="@+id/toolbar"
			app:srcCompat="@drawable/home"
			tools:ignore="MissingConstraints" />

		<TextView
			android:id="@+id/toolbarText"
			android:layout_width="60dp"
			android:layout_height="8dp"
			android:layout_gravity="center"
			android:layout_marginStart="16dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Dashboard"
			android:textColor="@android:color/black"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toStartOf="parent" />

		<ImageView
			android:id="@+id/imageView15"
			android:layout_width="16dp"
			android:layout_height="16dp"
			android:layout_marginBottom="4dp"
			app:layout_constraintBottom_toTopOf="@+id/textView13"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintHorizontal_bias="0.275"
			app:layout_constraintStart_toStartOf="parent"
			app:srcCompat="@drawable/moon" />

		<TextView
			android:id="@+id/textView13"
			android:layout_width="38dp"
			android:layout_height="9dp"
			android:layout_marginStart="22dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Tasks"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintStart_toEndOf="@+id/toolbarText" />


		<TextView
			android:id="@+id/textView14"
			android:layout_width="53dp"
			android:layout_height="12dp"
			android:layout_marginEnd="8dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Habits"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/textView15" />

		<ImageView
			android:id="@+id/imageView17"
			android:layout_width="15dp"
			android:layout_height="16dp"
			android:layout_marginStart="116dp"
			android:layout_marginBottom="3dp"
			app:layout_constraintBottom_toTopOf="@+id/textView14"
			app:layout_constraintStart_toStartOf="@+id/imageView16"
			app:srcCompat="@drawable/habituals" />

		<TextView
			android:id="@+id/textView15"
			android:layout_width="44dp"
			android:layout_height="12dp"
			android:layout_marginEnd="28dp"
			android:layout_marginBottom="16dp"
			android:gravity="center"
			android:text="Expense"
			android:textSize="18px"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent" />

		<ImageView
			android:id="@+id/imageView18"
			android:layout_width="15dp"
			android:layout_height="16dp"
			android:layout_marginEnd="44dp"
			android:layout_marginBottom="32dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:srcCompat="@drawable/expense" />


	</androidx.constraintlayout.widget.ConstraintLayout>

	<ImageView
		android:id="@+id/imageView27"
		android:layout_width="37dp"
		android:layout_height="18dp"
		android:layout_marginTop="16dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.922"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/bell" />

	<RelativeLayout
		android:id="@+id/groupedContainer"
		android:layout_width="411dp"
		android:layout_height="124dp"
		android:layout_margin="16dp"
		app:layout_constraintBottom_toTopOf="@+id/groupedContainer2"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		tools:ignore="MissingConstraints">


		<ImageView
			android:id="@+id/rectangleBackground"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#F5BB66"
			android:scaleType="fitXY" />

		<TextView
			android:id="@+id/textView32"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="33dp"
			android:layout_marginTop="18dp"
			android:text="Expense" />

		<TextView
			android:id="@+id/textView33"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="31dp"
			android:layout_marginTop="77dp"
			android:text="P 100.00" />

	</RelativeLayout>

	<RelativeLayout
		android:id="@+id/groupedContainer2"
		android:layout_width="320dp"
		android:layout_height="201dp"
		android:layout_margin="16dp"
		android:layout_marginStart="46dp"
		android:layout_marginEnd="75dp"
		app:layout_constraintBottom_toBottomOf="@+id/constraintLayout2"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:ignore="MissingConstraints">


		<ImageView
			android:id="@+id/rectangleBackground2"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#F5BB66"
			android:scaleType="fitXY" />

		<TextView
			android:id="@+id/textView40"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="20dp"
			android:layout_marginTop="14dp"
			android:text="Amount:" />

		<TextView
			android:id="@+id/textView34"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="19dp"
			android:layout_marginTop="123dp"
			android:text="Type:" />

		<TextView
			android:id="@+id/textView41"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentStart="true"
			android:layout_alignParentTop="true"
			android:layout_marginStart="19dp"
			android:layout_marginTop="64dp"
			android:text="Note:" />

		<EditText
			android:id="@+id/editTextText2"
			android:layout_width="123dp"
			android:layout_height="29dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_marginTop="6dp"
			android:layout_marginEnd="24dp"
			android:ems="10"
			android:inputType="text"
			android:text=""
			app:layout_constraintEnd_toEndOf="parent" />


		<EditText
			android:id="@+id/editTextText3"
			android:layout_width="120dp"
			android:layout_height="35dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_marginTop="113dp"
			android:layout_marginEnd="27dp"
			android:ems="10"
			android:inputType="text"
			android:text=""
			android:visibility="visible"
			app:layout_constraintEnd_toEndOf="parent" />

		<Spinner
			android:id="@+id/spinner3"
			android:layout_width="112dp"
			android:layout_height="31dp"
			android:layout_alignParentTop="true"
			android:layout_alignParentEnd="true"
			android:layout_marginTop="58dp"
			android:layout_marginEnd="26dp" />

		<Button
			android:id="@+id/button4"
			android:layout_width="wrap_content"
			android:layout_height="33dp"
			android:layout_alignParentStart="true"
			android:layout_alignParentBottom="true"
			android:layout_marginStart="52dp"
			android:layout_marginBottom="5dp"
			android:background="@drawable/mybutton"
			android:text="Cancel"
			android:textSize="30px" />

		<Button
			android:id="@+id/button6"
			android:layout_width="wrap_content"
			android:layout_height="33dp"
			android:layout_alignParentStart="true"
			android:layout_alignParentBottom="true"
			android:layout_marginStart="172dp"
			android:layout_marginBottom="5dp"
			android:background="@drawable/mybutton"
			android:text="Save"
			android:textSize="30px" />

	</RelativeLayout>

	<ImageView
		android:id="@+id/imageView29"
		android:layout_width="35dp"
		android:layout_height="33dp"
		android:layout_marginEnd="28dp"
		android:layout_marginBottom="24dp"
		app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
		app:layout_constraintEnd_toEndOf="parent"
		app:srcCompat="@drawable/right" />


</androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>